{
  "name": "watson-artist-lyrics-analysis",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cfenv": "1.0.x",
    "cheerio": "^1.0.0-rc.2",
    "d3": "^4.12.2",
    "d3-jetpack": "^2.0.16",
    "express": "4.15.x",
    "mongodb": "^3.0.4",
    "node-sass-chokidar": "^0.0.3",
    "nodemailer": "^4.4.2",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "watson-developer-cloud": "^2.42.0"
  },
  "repository": {},
  "engines": {
    "node": "8.x"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/static/styles/sass --include-path ./node_modules ./src/static/styles/sass -o src/static/styles",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/static/styles/sass --include-path ./node_modules ./src/static/styles/sass -o src/static/styles --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "mocha",
    "test-react": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "expect.js": "^0.3.1",
    "mocha": "^4.0.1",
    "superagent": "^3.5.2"
  }
}
